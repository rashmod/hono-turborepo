{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "start": {
      "dependsOn": ["^start"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "inputs": ["./**/*.{js,jsx,ts,tsx}"],
      "outputs": ["./**/*.{js,jsx,ts,tsx}"]
    },
    "clean": {},
    "clean:turbo": {}
  }
}
